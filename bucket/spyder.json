{
    "version": "5.3.1",
    "description": "Spyder is a powerful scientific environment written in Python, for Python, and designed by and for scientists, engineers and data analysts",
    "homepage": "https://www.spyder-ide.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/spyder-ide/spyder/releases/download/v5.3.1/Spyder_64bit_full.exe",
            "hash": "820a38b1de9c25c021dbcfed03ab0eff3e9b7341c819ced05fb327680137d7e4"
        }
    },
    "installer": {
        "args": [
            "/S",
            "/InstallationType=JustMe",
            "/RegisterPython=0",
            "/AddToPath=0",
            "/NoRegistry=1",
            "/D=$dir"
        ]
    },
    "uninstaller": {
        "file": "Uninstall-Miniforge3.exe",
        "args": "/S"
    },
    "bin": [
        "python.exe",
        "pythonw.exe",
        [
            "python.exe",
            "python3"
        ]
    ],
    "env_add_path": [
        "scripts",
        "Library\\bin"
    ],
    "persist": "envs",
    "checkver": {
        "url": "https://github.com/spyder-ide/spyder/releases/latest",
        "regex": "Spyder (?\\d\\.+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/spyder-ide/spyder/releases/download/v$version/Spyder_64bit_full.exe"
            }
        }
    }
}
